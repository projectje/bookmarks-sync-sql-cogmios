CREATE TABLE IF NOT EXISTS Url
(id INTEGER PRIMARY KEY AUTOINCREMENT,
 url TEXT,
 UNIQUE(url));

CREATE TABLE IF NOT EXISTS User
 (id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT,
  UNIQUE(name));

CREATE TABLE IF NOT EXISTS Location
 (id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT,
  UNIQUE(name));

CREATE TABLE IF NOT EXISTS Root
 (id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT,
  UNIQUE(name));

CREATE TABLE IF NOT EXISTS Name
(id INTEGER PRIMARY KEY AUTOINCREMENT,
 urlId INTEGER,
 userId INTEGER,
 locationId INTEGER,
 rootId INTEGER,
 path TEXT,
 name TEXT,
 CONSTRAINT urlid_fk_Name FOREIGN KEY (urlId) REFERENCES Url(id) ON UPDATE CASCADE ON DELETE CASCADE);
